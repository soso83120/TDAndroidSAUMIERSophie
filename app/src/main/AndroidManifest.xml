<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tdandroidsaumiersophie">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:color="@color/black"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TDAndroidSAUMIERSophie"
        android:usesCleartextTraffic="true">
        <activity android:name=".registre.LoginActivity"></activity>
        <activity android:name=".registre.RegisterActivity" />
        <activity android:name=".basket.BasketActivity" />
        <activity android:name=".detail.DetailActivity" />
        <activity android:name=".Category.CategoryActivity" />
        <activity android:name=".network.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>